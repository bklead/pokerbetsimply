@model PokerBet.Areas.AdminPanel.Models.LogOnModel

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Start</title>
    <link href="/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="/Content/admin.css" rel="stylesheet" type="text/css" />
    <script src="/Scripts/jquery-1.5.1.min.js" type="text/javascript"></script>
    <script src="/Scripts/jquery.validate.min.js" type="text/javascript"></script>
    <script src="/Scripts/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
</head>

<body onload="resize()" style="overflow:auto;background: #075A08;">
        <div class="tablesa" style="margin: 40px auto; width: 300px;">

        @using (Html.BeginForm())
        {        
            @Html.ValidationSummary(true)

		    <div>
                @Html.LabelFor(m => m.UserName)
                @Html.EditorFor(m => m.UserName)
            </div>
            
            <div>
                @Html.LabelFor(m => m.Password)
                @Html.EditorFor(m => m.Password)
            </div> 
            
            <input type="submit" id="start" name="start" value="Start" />          
        }

    </div>
</body>
</html>